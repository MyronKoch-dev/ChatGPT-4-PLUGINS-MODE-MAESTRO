{
  "manifest_version": 3,
  "name": "ChatGPT PLUS | MODE MAESTRO",
  "version": "1.0",
  "description": "Quickly access any mode of ChatGPT Plus: GPT-4 + Plugins, GPT-4 Default, or GPT-3.5 Default. No more detours!",
  "permissions": [],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*"],
      "js": ["contentScript.js"]
    }
  ]
}








